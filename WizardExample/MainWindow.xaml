<Window x:Class="WizardExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Canvas x:Name="wizardHost" Margin="12,12,12,41">
        </Canvas>
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="416,276,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" Command="{Binding WizardVM.CancelCommand}"  />
        <Button Content="Next" Height="23" HorizontalAlignment="Left" Margin="335,276,0,0" Name="button2" VerticalAlignment="Top" Width="75" Command="{Binding WizardVM.MoveNextCommand}" />
        <Button Content="Back" Height="23" HorizontalAlignment="Left" Margin="254,276,0,0" Name="button3" VerticalAlignment="Top" Width="75" Command="{Binding WizardVM.MovePreviousCommand}" />
    </Grid>
</Window>
